            function drawCanvasOne() {
                var t = document.getElementById("wave"),
                    e = echarts.init(t),
                    i = e.getZr(),
                    a = e.getWidth(),
                    c = e.getHeight(),
                    o = new echarts.graphic.Group;
                i.add(o);
                var n = 3;
                800 > a && (n = 2);
                for (var r = 0; 3 > r; r++) {
                    for (var s = [], l = 0; n + 1 >= l; l++) {
                        var h = c / 10 * r + c / 6,
                            d = Math.random() * c / 8 + h,
                            g = c - Math.random() * c / 8 - h,
                            m = [
                                [2 * l * a / n / 2, r % 2 ? d : g],
                                [(2 * l + 1) * a / n / 2, r % 2 ? g : d]
                            ];
